{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "fatbeehive/fb-drupal8-modules1",
                "version": "dev-master",
                "type": "package",
                "source": {
                    "url": "git@bitbucket.org:fatbeehive/fb-drupal8-modules1.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fatbeehive/fb-drupal8-installation-profile",
                "version": "dev-master",
                "type": "package",
                "source": {
                    "url": "git@bitbucket.org:fatbeehive/fb-drupal8-installation-profile.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/previousnext/drush_cmi_tools"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webmozart/path-util": "^2.3",
        "drupal/config_update": "^1.2",
        "drupal/eu_cookie_compliance": "^1.0",
        "drupal/config_readonly": "^1.0",
        "drupal/ckeditor_media_embed": "^1.0",
        "drupal/webform": "^5.0",
        "drupal/menu_block": "^1.4",
        "drupal/adminimal_admin_toolbar": "^1.3",
        "drupal/admin_toolbar": "^1.17",
        "drupal/editor_advanced_link": "^1.3",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/contact_storage_export": "^1.7",
        "drupal/mailsystem": "^4.1",
        "drupal/weight": "^3.0",
        "drupal/metatag": "^1.0",
        "drupal/crop": "^1.0",
        "drupal/twig_tweak": "^1.0",
        "drupal/focal_point": "^1.0",
        "drupal/sitemap": "^1.2",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/taxonomy_access_fix": "^2.0",
        "drupal/role_delegation": "^1.0",
        "drupal/google_analytics": "^2.1",
        "drupal/workbench": "^1.0",
        "drupal/devel": "^1.0",
        "drupal/honeypot": "^1.23",
        "drupal/token": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/video_embed_field": "^1.3",
        "drupal/imce": "^1.4",
        "drupal/linkit": "^4.1",
        "drupal/xmlsitemap": "^1.0",
        "drupal/smtp": "^1.0",
        "drupal/workbench_moderation": "^1.1",
        "drupal/redirect": "^1.0",
        "drupal/swiftmailer": "^1.0",
        "drupal/adminimal_theme": "^1.2",
        "drupal/block_visibility_groups": "^1.1",
        "drupal/paragraphs": "^1.0",
        "drupal/contact_storage": "^1.0",
        "drupal/contact_emails": "^1.0",
        "drupal/simple_user_management": "^1.0",
        "drupal/site_settings": "^1.0",
        "drupal/chosen": "^2.0",
        "drupal/menu_admin_per_menu": "^1.0"
    },
    "require-dev": {
        "fatbeehive/fb-drupal8-modules1": "dev-master",
        "fatbeehive/fb-drupal8-installation-profile": "dev-master",
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/fb": ["fatbeehive/fb-drupal8-modules1"],
            "web/profiles/contrib/fatbeehive": ["fatbeehive/fb-drupal8-installation-profile"]
        }
    }
}
